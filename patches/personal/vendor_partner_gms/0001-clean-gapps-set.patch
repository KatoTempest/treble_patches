From: Victor Bo <bvoid@yandex.ru>
Date: Sun, 13 Dec 2020 19:21:02 -0500
Subject: [PATCH] clean gapps set

---
 products/gms.mk    | 34 ++--------------------------------
 products/gms_go.mk | 32 ++------------------------------
 2 files changed, 4 insertions(+), 62 deletions(-)

diff --git a/products/gms.mk b/products/gms.mk
index b50f18c..779e0de 100644
--- a/products/gms.mk
+++ b/products/gms.mk
@@ -15,22 +15,16 @@ PRODUCT_PACKAGES := \
     AndroidPlatformServices \
     ConfigUpdater \
     GoogleExtShared \
-    GoogleFeedback \
     GoogleLocationHistory \
     GoogleOneTimeInitializer \
-    GooglePackageInstaller \
-    GooglePartnerSetup \
     GooglePrintRecommendationService \
     GoogleRestore \
     GoogleServicesFramework \
     GoogleCalendarSyncAdapter \
     GoogleContactsSyncAdapter \
-    GoogleTTS \
     GmsCore \
     Phonesky \
-    SetupWizard \
-    WebViewGoogle \
-    Wellbeing
+    WebViewGoogle
 
 # GMS common RRO packages
 PRODUCT_PACKAGES += GmsConfigOverlayCommon GmsConfigOverlayGSA
@@ -53,35 +47,11 @@ PRODUCT_PACKAGE_OVERLAYS += $(ANDROID_PARTNER_GMS_HOME)/overlay/gms_overlay
 
 # GMS mandatory application packages
 PRODUCT_PACKAGES += \
-    Chrome \
-    Drive \
-    Gmail2 \
-    Duo \
-    Maps \
-    YTMusic \
-    Photos \
-    Velvet \
-    Videos \
-    YouTube
+    Velvet
 
 # GMS comms suite
 $(call inherit-product, $(ANDROID_PARTNER_GMS_HOME)/products/google_comms_suite.mk)
 
-# GMS optional application packages
-PRODUCT_PACKAGES += \
-    CalendarGoogle \
-    DeskClockGoogle \
-    LatinImeGoogle \
-    TagGoogle \
-    talkback \
-    Keep \
-    CalculatorGoogle
-
-# GMS sample application packages
-PRODUCT_PACKAGES += \
-    GmsSampleIntegration \
-    SearchLauncherQuickStep
-
 PRODUCT_PRODUCT_PROPERTIES += \
     ro.setupwizard.rotation_locked=true \
     setupwizard.theme=glif_v3_light \
diff --git a/products/gms_go.mk b/products/gms_go.mk
index 69ab5ef..2061c7e 100644
--- a/products/gms_go.mk
+++ b/products/gms_go.mk
@@ -14,23 +14,17 @@ PRODUCT_PACKAGES := \
     AndroidPlatformServices \
     ConfigUpdater \
     GoogleExtShared \
-    GoogleFeedback \
     GoogleLocationHistory \
     GoogleOneTimeInitializer \
-    GooglePackageInstaller \
-    GooglePartnerSetup \
     GooglePrintRecommendationService \
     GoogleRestore \
     GoogleServicesFramework \
     GoogleSpeechServices \
     GoogleCalendarSyncAdapter \
     GoogleContactsSyncAdapter \
-    GoogleTTS \
     GmsCoreGo \
     Phonesky \
-    SetupWizard \
-    WebViewGoogle \
-    Wellbeing
+    WebViewGoogle
 
 # GMS common RRO packages
 PRODUCT_PACKAGES += GmsConfigOverlayCommon GmsConfigOverlayGo GmsConfigOverlayAssistantGo
@@ -56,33 +50,11 @@ PRODUCT_PACKAGE_OVERLAYS += $(ANDROID_PARTNER_GMS_HOME)/overlay/gms_overlay
 # GMS mandatory application packages
 PRODUCT_PACKAGES += \
     AssistantGo \
-    Chrome \
-    DuoGo \
-    GalleryGo \
-    GMailGo \
-    GoogleSearchGo \
-    LatinImeGoogleGo \
-    MapsGo \
-    NavGo \
-    YouTube
+    GoogleSearchGo
 
 # GMS comms suite
 $(call inherit-product, $(ANDROID_PARTNER_GMS_HOME)/products/google_go_comms_suite.mk)
 
-# GMS optional application packages
-PRODUCT_PACKAGES += \
-    CalendarGoogle \
-    DeskClockGoogle \
-    FilesGoogle \
-    TagGoogle \
-    talkback \
-    CalculatorGoogle
-
-# GMS sample application packages
-PRODUCT_PACKAGES += \
-    GmsSampleIntegrationGo \
-    Launcher3Go
-
 PRODUCT_PRODUCT_PROPERTIES += \
     ro.setupwizard.rotation_locked=true \
     setupwizard.theme=glif_v3_light \
