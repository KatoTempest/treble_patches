From 6c07ace25021a13bc5f49c0504f41461bda10c3d Mon Sep 17 00:00:00 2001
From: Victor Bo <bvoid@yandex.ru>
Date: Sun, 13 Dec 2020 19:21:02 -0500
Subject: [PATCH] clean gapps set v2

---
 products/gms.mk    | 33 +++------------------------------
 products/gms_go.mk | 32 ++------------------------------
 2 files changed, 5 insertions(+), 60 deletions(-)

diff --git a/products/gms.mk b/products/gms.mk
index b50f18c..502c731 100644
--- a/products/gms.mk
+++ b/products/gms.mk
@@ -15,22 +15,15 @@ PRODUCT_PACKAGES := \
     AndroidPlatformServices \
     ConfigUpdater \
     GoogleExtShared \
-    GoogleFeedback \
     GoogleLocationHistory \
     GoogleOneTimeInitializer \
-    GooglePackageInstaller \
-    GooglePartnerSetup \
     GooglePrintRecommendationService \
     GoogleRestore \
     GoogleServicesFramework \
     GoogleCalendarSyncAdapter \
     GoogleContactsSyncAdapter \
-    GoogleTTS \
     GmsCore \
-    Phonesky \
-    SetupWizard \
-    WebViewGoogle \
-    Wellbeing
+    Phonesky
 
 # GMS common RRO packages
 PRODUCT_PACKAGES += GmsConfigOverlayCommon GmsConfigOverlayGSA
@@ -53,34 +46,14 @@ PRODUCT_PACKAGE_OVERLAYS += $(ANDROID_PARTNER_GMS_HOME)/overlay/gms_overlay
 
 # GMS mandatory application packages
 PRODUCT_PACKAGES += \
-    Chrome \
-    Drive \
-    Gmail2 \
-    Duo \
-    Maps \
-    YTMusic \
-    Photos \
-    Velvet \
-    Videos \
-    YouTube
+    Velvet
 
 # GMS comms suite
 $(call inherit-product, $(ANDROID_PARTNER_GMS_HOME)/products/google_comms_suite.mk)
 
 # GMS optional application packages
 PRODUCT_PACKAGES += \
-    CalendarGoogle \
-    DeskClockGoogle \
-    LatinImeGoogle \
-    TagGoogle \
-    talkback \
-    Keep \
-    CalculatorGoogle
-
-# GMS sample application packages
-PRODUCT_PACKAGES += \
-    GmsSampleIntegration \
-    SearchLauncherQuickStep
+    LatinImeGoogle
 
 PRODUCT_PRODUCT_PROPERTIES += \
     ro.setupwizard.rotation_locked=true \
diff --git a/products/gms_go.mk b/products/gms_go.mk
index 69ab5ef..81ab212 100644
--- a/products/gms_go.mk
+++ b/products/gms_go.mk
@@ -14,23 +14,16 @@ PRODUCT_PACKAGES := \
     AndroidPlatformServices \
     ConfigUpdater \
     GoogleExtShared \
-    GoogleFeedback \
     GoogleLocationHistory \
     GoogleOneTimeInitializer \
-    GooglePackageInstaller \
-    GooglePartnerSetup \
     GooglePrintRecommendationService \
     GoogleRestore \
     GoogleServicesFramework \
     GoogleSpeechServices \
     GoogleCalendarSyncAdapter \
     GoogleContactsSyncAdapter \
-    GoogleTTS \
     GmsCoreGo \
-    Phonesky \
-    SetupWizard \
-    WebViewGoogle \
-    Wellbeing
+    Phonesky
 
 # GMS common RRO packages
 PRODUCT_PACKAGES += GmsConfigOverlayCommon GmsConfigOverlayGo GmsConfigOverlayAssistantGo
@@ -56,33 +49,12 @@ PRODUCT_PACKAGE_OVERLAYS += $(ANDROID_PARTNER_GMS_HOME)/overlay/gms_overlay
 # GMS mandatory application packages
 PRODUCT_PACKAGES += \
     AssistantGo \
-    Chrome \
-    DuoGo \
-    GalleryGo \
-    GMailGo \
-    GoogleSearchGo \
     LatinImeGoogleGo \
-    MapsGo \
-    NavGo \
-    YouTube
+    GoogleSearchGo
 
 # GMS comms suite
 $(call inherit-product, $(ANDROID_PARTNER_GMS_HOME)/products/google_go_comms_suite.mk)
 
-# GMS optional application packages
-PRODUCT_PACKAGES += \
-    CalendarGoogle \
-    DeskClockGoogle \
-    FilesGoogle \
-    TagGoogle \
-    talkback \
-    CalculatorGoogle
-
-# GMS sample application packages
-PRODUCT_PACKAGES += \
-    GmsSampleIntegrationGo \
-    Launcher3Go
-
 PRODUCT_PRODUCT_PROPERTIES += \
     ro.setupwizard.rotation_locked=true \
     setupwizard.theme=glif_v3_light \
