From 18cc325d97e5a932a7c009ed1ec67d126bc263b9 Mon Sep 17 00:00:00 2001
From: Victor Bo <bvoid@yandex.ru>
Date: Wed, 21 Oct 2020 13:58:06 -0400
Subject: [PATCH 10/11] tweaks for Xiaomi Redmi 9 (Prime) 'lancelot'

  from
  https://forum.xda-developers.com/showpost.php?p=83640257&postcount=365
---
 rw-system.sh | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/rw-system.sh b/rw-system.sh
index 719ea49..f49267f 100644
--- a/rw-system.sh
+++ b/rw-system.sh
@@ -317,8 +317,8 @@ if getprop ro.vendor.build.fingerprint | grep -q -i \
     setprop persist.sys.qcom-brightness "$(cat /sys/class/leds/lcd-backlight/max_brightness)"
 fi
 
-#Xiaomi Redmi Note 9
-if getprop ro.vendor.build.fingerprint |grep -qi -e Redmi/merlin; then
+#Xiaomi Redmi Note 9, Redmi 9 (Prime)
+if getprop ro.vendor.build.fingerprint |grep -qi -e Redmi/merlin -e Redmi/lancelot; then
     setprop persist.sys.overlay.devinputjack true
 fi
 
