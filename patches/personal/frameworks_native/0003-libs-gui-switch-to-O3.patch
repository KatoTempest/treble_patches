From bc068c006fa1f889b915162eb48edd74e6bf20dc Mon Sep 17 00:00:00 2001
From: Victor Bo <bvoid@yandex.ru>
Date: Wed, 24 Mar 2021 08:21:36 -0400
Subject: [PATCH 03/12] libs: gui: switch to -O3

---
 libs/gui/Android.bp | 5 +++++
 1 file changed, 5 insertions(+)

diff --git a/libs/gui/Android.bp b/libs/gui/Android.bp
index ea22f05f6..b318c1f5f 100644
--- a/libs/gui/Android.bp
+++ b/libs/gui/Android.bp
@@ -98,6 +98,7 @@ cc_library_shared {
     target: {
         vendor: {
             cflags: [
+                "-O3",
                 "-DNO_BUFFERHUB",
                 "-DNO_INPUT",
             ],
@@ -141,6 +142,7 @@ cc_library_static {
     min_sdk_version: "29",
 
     cflags: [
+        "-O3",
         "-DNO_BUFFERHUB",
         "-DNO_BINDER",
     ],
@@ -181,11 +183,13 @@ cc_defaults {
 
     clang: true,
     cflags: [
+        "-O3",
         "-Wall",
         "-Werror",
     ],
 
     cppflags: [
+        "-O3",
         "-Wextra",
         "-DDEBUG_ONLY_CODE=0",
     ],
@@ -193,6 +197,7 @@ cc_defaults {
     product_variables: {
         eng: {
             cppflags: [
+                "-O3",
                 "-UDEBUG_ONLY_CODE",
                 "-DDEBUG_ONLY_CODE=1",
             ],
